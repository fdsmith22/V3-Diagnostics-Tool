{
  "name": "v3-diagnostics-tool",
  "version": "1.0.0",
  "description": "A lightweight, local web diagnostic interface for V3 sensor modules. Designed to quickly test returned units over USB connection.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx tailwindcss -i ./src/input.css -o ./static/styles.css --watch",
    "build": "npx tailwindcss -i ./src/input.css -o ./static/styles.css --minify",
    "watch:css": "postcss src/input.css -o static/styles.css --watch",
    "build:production": "NODE_ENV=production npm run build && gzip -9 ./static/styles.css"
  },
  "dependencies": {
    "alpinejs": "^3.12.0",
    "browserslist": "^4.24.5",
    "caniuse-lite": "^1.0.30001718",
    "electron-to-chromium": "^1.5.157",
    "escalade": "^3.2.0",
    "fraction.js": "^4.3.7",
    "nanoid": "^3.3.11",
    "node-releases": "^2.0.19",
    "normalize-range": "^0.1.2",
    "picocolors": "^1.1.1",
    "postcss-value-parser": "^4.2.0",
    "socket.io-client": "^4.7.2",
    "source-map-js": "^1.2.1",
    "update-browserslist-db": "^1.1.3"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.7.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "keywords": [
    "diagnostics",
    "v3",
    "sensor",
    "monitoring",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
